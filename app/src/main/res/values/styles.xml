<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="img_fb">
        <item name="android:layout_width">76dp</item>
        <item name="android:layout_height">74dp</item>
        <item name="android:layout_alignTop">@+id/imageView2</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="img_twitter">
        <item name="android:layout_width">76dp</item>
        <item name="android:layout_height">74dp</item>
        <item name="android:layout_above">@+id/textView</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_marginBottom">-107dp</item>
        <item name="android:layout_marginStart">55dp</item>
    </style>

    <style name="img_insta">
        <item name="android:layout_width">76dp</item>
        <item name="android:layout_height">74dp</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_alignTop">@+id/imageView</item>
        <item name="android:layout_marginEnd">52dp</item>
    </style>

    <style name="text_fb">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginStart">64dp</item>
        <item name="android:layout_marginTop">117dp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="text_twitter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignTop">@+id/textView</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="text_insta">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignEnd">@+id/imageView3</item>
        <item name="android:layout_alignTop">@+id/textView</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="view_one">
        <item name="android:layout_width">360dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">157dp</item>
        <item name="android:background">#000000</item>
    </style>

    <style name="quote_one">
        <item name="android:layout_width">189dp</item>
        <item name="android:layout_height">155dp</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_marginLeft">10sp</item>
        <item name="android:layout_marginTop">175dp</item>
        <item name="android:background">@color/box_1</item>
        <item name="android:paddingLeft">10sp</item>
        <item name="android:paddingRight">7sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="quote_two">
        <item name="android:layout_width">189dp</item>
        <item name="android:layout_height">207dp</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_marginBottom">23dp</item>
        <item name="android:layout_marginLeft">10sp</item>
        <item name="android:background">@color/box_2</item>
        <item name="android:paddingLeft">10sp</item>
        <item name="android:paddingRight">7sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="linear_one">
        <item name="android:layout_width">154dp</item>
        <item name="android:layout_height">106dp</item>
        <item name="android:layout_alignTop">@+id/textView4</item>
        <item name="android:layout_toEndOf">@+id/textView2</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="img_one">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/box_2</item>
    </style>

    <style name="linear_two">
        <item name="android:layout_width">156dp</item>
        <item name="android:layout_height">253dp</item>
        <item name="android:layout_alignBottom">@+id/textView7</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_marginEnd">21dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="img_two">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">103dp</item>
    </style>

    <style name="view_two">
        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">#000000</item>
    </style>

    <style name="eiffel_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:background">@color/box_1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="eiffel_content">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">114dp</item>
        <item name="android:background">@color/box_1</item>
        <item name="android:paddingLeft">5sp</item>
        <item name="android:paddingRight">5sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="relative_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/bg</item>
    </style>

</resources>
